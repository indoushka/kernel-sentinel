[Unit]
Description=Kernel Sentinel Security Update Service By indoushka
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
ExecStart=/usr/bin/python3 /usr/lib/kernel-sentinel/core.py --تحديث
User=root
Environment=LANG=en_US.UTF-8
Environment=LC_ALL=en_US.UTF-8
WorkingDirectory=/usr/lib/kernel-sentinel
StandardOutput=journal
StandardError=journal
SyslogIdentifier=kernel-sentinel
